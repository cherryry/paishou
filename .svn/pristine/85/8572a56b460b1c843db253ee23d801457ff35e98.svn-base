/* pages/my_coupons/my_coupons.wxss */
/*顶部  */


.swiper-tab {
  width: 100%;
  border-top: 1rpx solid #e2e2e2;
  border-bottom: 1rpx solid #e2e2e2;
  text-align: center;
  height: 80rpx;
  line-height: 76rpx;
  background: #fff;
}

.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 30%;
  color: #777;
  text-align: center;
}
.on {
  border-bottom: 5rpx solid #64cb97;
}

/*优惠券  */

.couponBox {
  width: 100%;
  background-color: #f8f8fa;
  height: 235rpx;
  position: relative;
  z-index: 1;
}

.coupon {
  width: 95%;
  margin: 0 auto;
  padding-top: 25rpx;
  position: relative;
  z-index: 2;
}

.couponImg {
  width: 100%;
  height: 200rpx;
  position: absolute;
}

.couponContent {
  position: absolute;
  z-index: 3;
  height: 200rpx;
  width: 100%;
}

.btn {
  background-color: #fff;
  border-radius: 5rpx;
  font-size: 24rpx;
  width: 160rpx;
  height: 50rpx;
  position: absolute;
  display: inline-block;
  top: 50%;
  margin-top: -35rpx;
  opacity: 0.6;
  font-weight: bolder;
  text-align: center;
  left: 70%;
}
.symbol{
  position: absolute;
  top: 45%;
  margin-top: -20rpx;
  left: 10%;
  font-size: 32rpx;
  font-weight: lighter;
}
.symbol2{
  position: absolute;
  top: 50%;
  margin-top: -20rpx;
  left: 29%;
  font-size: 20rpx;
  font-weight: lighter;
}
.moneyMany{
  position: absolute;
  left: 15%;
  top: 1%;
  font-size: 100rpx;
  font-weight: lighter;
  letter-spacing: 0;
}
.moneyMany2{
  position: absolute;
  left: 9%;
  top: 1%;
  font-size: 100rpx;
  font-weight: lighter;
  letter-spacing: 0;
}
.condition{
  font-size: 18rpx;
  position: absolute;
  top: 23%;
  left: 33%;
}
.couponText{
  position: absolute;
  top: 35%;
  left: 33%;
  font-size: 24rpx;
}
.couponText2{
  position: absolute;
  top: 35%;
  left: 33%;
  font-size: 28rpx;
}
.couponTime{
  font-size: 20rpx;
  position: absolute;
  top: 60%;
  left: 10%;
}